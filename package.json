{
  "name": "MC_RN",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npx react-native run-ios --simulator \"iPad Pro (12.9-inch) (5th generation)\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "cd cli && node ./index.js",
    "release-android": "git checkout main && npm run sign && source ~/.bash_profile && cd android && ./gradlew channelRelease && git checkout main-zt && ./gradlew channelRelease && git checkout main",
    "release-ios": "npx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output dist/main.jsbundle --assets-dest dist/",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "debug-release-android": "npx react-native run-android --variant=release",
    "publish-base-android": "pushy uploadApk android/app/build/outputs/apk/release/app-arm64-v8a-release.apk",
    "publish-base-ios": "pushy uploadIpa dist/ios.ipa",
    "postinstall": "patch-package",
    "sign": "cd android/app && ./make-new-sign.sh",
    "vi-sign": "vi ~/.gradle/gradle.properties"
  },
